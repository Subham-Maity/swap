{
  "parser": "@typescript-eslint/parser",
  "plugins": ["react-hooks", "@typescript-eslint"],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "prefer-const": "off"
  },
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"]
}
